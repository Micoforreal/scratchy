# LLM Configuration
# Choose LLM provider: "openai" or "local"
LLM_PROVIDER=openai

# OpenAI Configuration (if using OpenAI provider)
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4o-mini
OPENAI_BASE_URL=https://api.openai.com/v1

# Local LLM Configuration (if using local provider)
LOCAL_LLM_BASE_URL=http://localhost:11434
LOCAL_LLM_MODEL=llama3

# Optional: Self-hosted crawler
CRAWLER_BASE_URL=http://localhost:8000
CRAWLER_ENABLED=false

# GitHub API (optional - for real data collection)
GITHUB_TOKEN=your-github-token-here

# Solana RPC (optional - for real onchain data)
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
